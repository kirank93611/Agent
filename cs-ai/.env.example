# Frontend Environment Variables

# API Configuration
REACT_APP_API_URL=http://localhost:5000
REACT_APP_API_VERSION=v1

# Authentication
REACT_APP_AUTH_TOKEN_KEY=auth_token
REACT_APP_REFRESH_TOKEN_KEY=refresh_token

# OpenAI Configuration (for AI Chat)
REACT_APP_OPENAI_API_KEY=your_openai_api_key_here
REACT_APP_MODEL_NAME=gpt-4-turbo-preview

# Feature Flags
REACT_APP_ENABLE_AI_CHAT=true
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_NOTIFICATIONS=true

# Third Party Services
REACT_APP_SENTRY_DSN=your_sentry_dsn_here
REACT_APP_GA_TRACKING_ID=your_ga_tracking_id_here

# Misc
REACT_APP_APP_NAME=CS-AI
REACT_APP_SUPPORT_EMAIL=support@example.com

# Deployment Environment
REACT_APP_ENV=development # development, staging, production 